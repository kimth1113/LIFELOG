{
  "name": "jeju-log-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "dev:front": "npm run dev --workspace=frontend",
    "dev:back": "cd backend && python manage.py runserver 0.0.0.0:8000",
    "build": "npm run build --workspace=frontend",
    "install:all": "npm install && cd backend && pip install -r requirements.txt",
    "start": "echo 'Use npm run dev for local development'"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
